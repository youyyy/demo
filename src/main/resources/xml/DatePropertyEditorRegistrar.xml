<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">
    <bean class="org.springframework.beans.factory.config.CustomEditorConfigurer">
        <property name="propertyEditorRegistrars">
            <list>
                <bean class="com.example.demo.bean.context.customDateEditor.DatePropertyEditorRegistrar"></bean>
            </list>
        </property>
    </bean>

    <bean id="dateType" class="com.example.demo.bean.context.datetype.DateType">
        <property name="dateValue" value="2020-04-28"/>
    </bean>


</beans>